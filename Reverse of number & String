x=int(input("Enter the number"))
rev=0
while(x>0):
    rem=x%10
    rev=(rev*10)+rem
    x=x//10

print("Reverse = %d"%rev)





def reverse(text):
    str=""
    i=len(text)-1
    print(i)
    while(i>=0):
        str=str+text[i]
        i=i-1
    return str


string=input("Enter the text")
print(reverse(string))
