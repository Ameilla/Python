py=int(input("Enter the marks in python: "))
c=int(input("Enter the marks in c programming: "))
mat=int(input("Enter the marks in mathematics: "))
phy=int(input("Enter the marks in physics: "))
total=py+c+mat+phy
tot=float(total)
aggregate=tot/4
print("Total= ",total)
print("Aggregate is =",aggregate)
if (aggregate >=75):
    print("DISTINCTION")
elif (aggregate >=60 and aggregate<75):
    print("First Division")
elif (aggregate >=50 and aggregate<60):
    print("Second Division")
elif (aggregate >=40 and aggregate<50):
    print("Third Division")
else:
    print("FAIL")

