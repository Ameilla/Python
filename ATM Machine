d1=int(input("Enter the 1st Denomination: "))
n1=int(input("Enter the 1st Denomination number of notes: "))
d2=int(input("Enter the 2nd Denomination: "))
n2=int(input("Enter the 2nd Denomination number of notes: "))
d3=int(input("Enter the 3rd Denomination: "))
n3=int(input("Enter the 3rd Denomination number of notes: "))
d4=int(input("Enter the 4th Denomination: "))
n4=int(input("Enter the 4th Denomination number of notes: "))
total=(d1*n1)+(d2*n2)+(d3*n3)+(d4*n4)
print("Total Available Balance in ATM: %d"%total)
